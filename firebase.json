{
  "hosting": {
    "target": "gonzalobos.com",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/es/**",
        "destination": "/index.html"
      },
      {
        "source": "/en/**",
        "destination": "/index.html"
      }
    ]
  }
}